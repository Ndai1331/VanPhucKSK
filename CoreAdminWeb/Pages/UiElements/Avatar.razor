@page "/avatar"
@inject IJSRuntime JsRuntime
<PageHeader Title="Elements" pTitle="Avatar"></PageHeader>

<!-- Start All Card -->
<div class="flex flex-col gap-4 min-h-[calc(100vh-212px)]">
    <div class="grid grid-cols-1 gap-4 md:grid-cols-2">
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar Rounded</MudText>
            <div class="flex flex-wrap items-center gap-4">
                <MudImage Src="assets/images/avatar-1.png" Class="flex-none object-cover rounded-full size-24" alt="avatar" />
                <MudImage Src="assets/images/avatar-4.png" Class="flex-none object-cover rounded-full size-20" alt="avatar" />
                <MudImage Src="assets/images/avatar-11.png" Class="flex-none object-cover rounded-full size-16" alt="avatar" />
                <MudImage Src="assets/images/avatar-13.png" Class="flex-none object-cover rounded-full size-12" alt="avatar" />
                <MudImage Src="assets/images/avatar-19.png" Class="flex-none object-cover rounded-full size-10" alt="avatar"/>
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar Square</MudText>
            <div class="flex flex-wrap items-center gap-4">
                <MudImage Src="assets/images/avatar-2.png" Class="flex-none object-cover rounded-md size-24" alt="avatar"/>
                <MudImage Src="assets/images/avatar-3.png" Class="flex-none object-cover rounded-md size-20" alt="avatar"/>
                <MudImage Src="assets/images/avatar-5.png" Class="flex-none object-cover rounded-md size-16" alt="avatar" />
                <MudImage Src="assets/images/avatar-6.png" Class="flex-none object-cover rounded-md size-12" alt="avatar" />
                <MudImage Src="assets/images/avatar-8.png" Class="flex-none object-cover rounded-md size-10" alt="avatar" />
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar With Dots</MudText>
            <div class="flex flex-wrap items-center gap-4">
                <div class="relative size-24">
                    <MudImage Src="assets/images/avatar-1.png" Class="flex-none object-cover rounded-full size-24" alt="avatar"/>
                    <span class="absolute bottom-0 rounded-full ltr:right-0 rtl:left-0 h-7 w-7 bg-danger ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-20">
                    <MudImage Src="assets/images/avatar-4.png" Class="flex-none object-cover rounded-full size-20" alt="avatar"/>
                    <span class="absolute bottom-0 rounded-full ltr:right-0 rtl:left-0 size-6 bg-warning ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-16">
                    <MudImage Src="assets/images/avatar-11.png" Class="flex-none object-cover rounded-full size-16" alt="avatar"/>
                    <span class="absolute bottom-0 rounded-full size-5 ltr:right-0 rtl:left-0 bg-success ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-12">
                    <MudImage Src="assets/images/avatar-13.png" Class="flex-none object-cover rounded-full size-12" alt="avatar"/>
                    <span class="absolute bottom-0 rounded-full ltr:right-0 rtl:left-0 size-4 bg-muted ring-2 ring-white dark:ring-darkborder"></span>
                </div>
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar Square With Dots</MudText>
            <div class="flex flex-wrap items-center gap-4">
                <div class="relative size-24">
                    <MudImage Src="assets/images/avatar-2.png" Class="flex-none object-cover rounded-md size-24" alt="avatar"/>
                    <span class="absolute rounded-full -bottom-2 ltr:-right-2 rtl:-left-2 size-7 bg-danger ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-20">
                    <MudImage Src="assets/images/avatar-3.png" Class="flex-none object-cover rounded-md size-20" alt="avatar"/>
                    <span class="absolute rounded-full size-6 -bottom-2 ltr:-right-2 rtl:-left-2 bg-warning ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-16">
                    <MudImage Src="assets/images/avatar-5.png" Class="flex-none object-cover rounded-md size-16" alt="avatar"/> 
                    <span class="absolute rounded-full size-5 -bottom-2 ltr:-right-2 rtl:-left-2 bg-success ring-2 ring-white dark:ring-darkborder"></span>
                </div>
                <div class="relative size-12">
                    <MudImage Src="assets/images/avatar-6.png" Class="flex-none object-cover rounded-md size-12" alt="avatar"/>
                    <span class="absolute rounded-full size-4 -bottom-2 ltr:-right-2 rtl:-left-2 bg-muted ring-2 ring-white dark:ring-darkborder"></span>
                </div>
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar Group</MudText>
            <div class="flex items-center justify-start -space-x-4 rtl:space-x-reverse">
                <MudImage Src="assets/images/avatar-2.png" Class="flex-none object-cover rounded-full size-10 ring-2 ring-white dark:ring-darkborder" alt="avatar"/>
                    <MudImage Src="assets/images/avatar-3.png" Class="flex-none object-cover rounded-full size-10 ring-2 ring-white dark:ring-darkborder" alt="avatar"/>
                        <MudImage Src="assets/images/avatar-5.png" Class="flex-none object-cover rounded-full size-10 ring-2 ring-white dark:ring-darkborder" alt="avatar"/>
                            <MudImage Src="assets/images/avatar-6.png" Class="flex-none object-cover rounded-full size-10 ring-2 ring-white dark:ring-darkborder" alt="avatar"/>
                                <MudImage Src="assets/images/avatar-8.png" Class="flex-none object-cover rounded-full size-10 ring-2 ring-white dark:ring-darkborder" alt="avatar"/>
                <span class="flex items-center justify-center object-cover text-xs font-bold text-center rounded-full size-10 text-white bg-muted ring-2 ring-white dark:ring-darkborder">+99</span>
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar With Text</MudText>
            <div class="flex items-center space-x-2 rtl:space-x-reverse">
                <MudImage Class="rounded-full size-10" Src="assets/images/avatar-1.png" alt=""/>
                <div class="font-medium">
                    <MudText Typo="Typo.h6" Class="text-sm dark:text-white">Stevens Lackson</MudText>
                    <MudText Class="text-xs text-black/40 dark:text-darkmuted">Frontend Developer</MudText>
                </div>
            </div>
        </div>
        <div class="card">
            <MudText Typo="Typo.h2" Class="mb-4 text-base font-semibold capitalize  text-slate-800 dark:text-slate-100">Avatar Square With Alphabet</MudText>
            <div class="flex flex-wrap gap-4">
                <div class="flex items-center justify-center text-base font-semibold uppercase rounded-md size-12 bg-purple/20 text-purple">Sl</div>
                <div class="flex items-center justify-center text-base font-semibold uppercase rounded-md size-12 bg-info/20 text-info">Sl</div>
                <div class="flex items-center justify-center text-base font-semibold uppercase rounded-md size-12 bg-success/20 text-success">Sl</div>
                <div class="flex items-center justify-center text-base font-semibold uppercase rounded-md size-12 bg-warning/20 text-warning">Sl</div>
